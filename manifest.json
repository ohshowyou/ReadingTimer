{
    "name": "Reading Timer",
    "manifest_version": 2,
    "version": "0.1",
    "description": "Timer for reading words",
    "permissions": ["contextMenus", "tabs", "background", "http://*/*", "https://*/*"],
    "browser_action": {
        "default_icon": "icon.png"},
    "background": {
        "scripts": ["background.js"]
    },
    "options_page": "options.html"
}